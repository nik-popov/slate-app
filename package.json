    "seed": "node seed-database.js"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.16",
    "dotenv": "^17.2.3",
    "firebase": "^12.5.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.16",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
